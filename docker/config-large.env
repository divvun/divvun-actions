# Configuration for large build servers
# Source this file before running update.sh or use with update-cron.sh

# Container configuration
export INSTANCE_COUNT=2
export CONTAINER_PREFIX="builder-large-"
export QUEUE_TAGS="queue=linux,size=large"

# Memory limits for large servers
export MEMORY_RESERVATION="16g"
export MEMORY_LIMIT="32g"

# Required: Set your Buildkite agent token
# export BUILDKITE_AGENT_TOKEN="your-token-here"