# Configuration for standard build servers
# Source this file before running update.sh or use with update-cron.sh

# Container configuration
export INSTANCE_COUNT=4
export CONTAINER_PREFIX="builder-"
export QUEUE_TAGS="queue=linux"

# Memory limits
export MEMORY_RESERVATION="6g"
export MEMORY_LIMIT="24g"

# Required: Set your Buildkite agent token
# export BUILDKITE_AGENT_TOKEN="your-token-here"